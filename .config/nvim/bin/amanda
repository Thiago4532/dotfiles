#!/bin/bash

function get_proc_start() {
    cut -d' ' -f22 < /proc/$1/stat
}
starttime=$(get_proc_start $$)

sleep 0.5

for pid in "$@"
do
    if [ -f /proc/$pid/stat ] && [ $(get_proc_start $pid) -le $starttime ]
    then
        echo "[$(date '+%D %T')]: Killed $pid $(</proc/$pid/cmdline)" >> /tmp/amanda.log
        kill -15 $pid
    fi
done


echo $st
